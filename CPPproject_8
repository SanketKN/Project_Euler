#include <iostream>
#include <cmath>
#include <cstring>
using namespace std;

int main(void)
{char num[] = "";
 int i, j;
 unsigned long long int p = 1,a = 0;
 for(i = 0; i < strlen(num) - 12; i++)
   {p = 1;
    if(num[i] == '0')
      i++;
    for(j = i; j < (i + 13); j++)
      p *= (num[j] - '0');
    if(p > a)
	   a = p;}
 cout<<"The product is: "<<a;
  return 0;
}
